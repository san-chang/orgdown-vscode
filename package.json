{
  "name": "orgdown-vscode",
  "version": "0.1.4",
  "homepage": "https://vscode.orgdown.org",
  "publisher": "idears-org",
  "engines": {
    "vscode": "^1.102.0"
  },
  "license": "SEE LICENSE IN LICENSE",
  "displayName": "OrgDown",
  "description": "OrgMode is the only reason I still keep Emacs on my device; that's why this extension was created.",
  "categories": [
    "Other",
    "Notebooks",
    "Programming Languages",
    "Snippets"
  ],
  "keywords": [
    "Org",
    "org",
    "Org Down",
    "org down",
    "OrgDown",
    "orgdown",
    "Org Mode",
    "org mode",
    "OrgMode",
    "orgmode",
    "Org Mode (Emacs)",
    "org mode (Emacs)"
  ],
  "main": "./client/dist/extension.js",
  "qna": false,
  "icon": "assets/orgdown.png",
  "repository": "https://github.com/idears-org/orgdown-vscode",
  "activationEvents": [
    "onLanguage:org"
  ],
  "contributes": {
    "languages": [
      {
        "id": "org",
        "aliases": [
          "Org",
          "org",
          "Org Down",
          "org down",
          "OrgDown",
          "orgdown",
          "Org Mode",
          "org mode",
          "OrgMode",
          "orgmode",
          "Org Mode (Emacs)",
          "org mode (Emacs)"
        ],
        "extensions": [
          ".org"
        ],
        "icon": {
          "light": "assets/orgdown-unicorn.svg",
          "dark": "assets/orgdown-unicorn.svg"
        },
        "configuration": "./syntaxes/org.language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "org",
        "scopeName": "text.org",
        "path": "./syntaxes/org.tmLanguage.json",
        "embeddedLanguages": {
          "source.diff": "diff",
          "source.twig": "twig",
          "source.python": "python",
          "source.js": "javascript",
          "source.ts": "typescript",
          "source.tsx": "typescriptreact",
          "source.ruby": "ruby",
          "source.go": "go",
          "source.cpp": "cpp",
          "source.c": "c",
          "source.java": "java",
          "source.shell": "shellscript",
          "source.perl": "perl",
          "source.r": "r",
          "source.swift": "swift",
          "source.scala": "scala",
          "source.elixir": "elixir",
          "source.erlang": "erlang",
          "source.dart": "dart",
          "source.yaml": "yaml",
          "source.json": "json",
          "source.css": "css",
          "source.less": "less",
          "source.css.scss": "scss",
          "source.coffee": "coffeescript",
          "source.makefile": "makefile",
          "source.dockerfile": "dockerfile",
          "source.php": "php",
          "text.html.basic": "html",
          "text.xml": "xml",
          "text.tex.latex": "latex",
          "text.bibtex": "bibtex",
          "text.pug": "jade",
          "text.html.handlebars": "handlebars",
          "text.html.markdown": "markdown",
          "source.fsharp": "fsharp",
          "source.cs": "csharp",
          "source.objc": "objective-c"
        }
      }
    ]
  },
  "scripts": {
    "clean": "rm -rf out dist client/dist server/dist tsconfig.tsbuildinfo",
    "vscode:prepublish": "pnpm run build",
    "build:grammar": "tsx scripts/build-grammar.ts",
    "build:inspections": "tsx scripts/build-inspections.ts",
    "build:theme": "tsx scripts/build-theme.ts",
    "build:source": "node esbuild.js --production",
    "build": "pnpm run clean && pnpm run build:grammar && pnpm run build:source",
    "watch:source": "node esbuild.js --watch",
    "watch:grammar": "pnpm exec tsx scripts/watch-grammar.ts",
    "watch:inspections": "pnpm exec tsx scripts/watch-inspections.ts",
    "watch:test:unit": "vitest watch",
    "watch": "pnpm run watch:source & pnpm run watch:grammar & pnpm run watch:inspections & pnpm run watch:test:unit",
    "check-types": "tsc --noEmit",
    "lint": "eslint client/src server/src common/src scripts test/unit test/integration --ext .ts,.js,.mjs",
    "pretest": "pnpm run build:grammar && pnpm run build:inspections",
    "test:unit": "vitest run",
    "test:integration": "vscode-test",
    "test": "pnpm run test:unit && pnpm run test:integration"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/js-yaml": "^4.0.9",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.102.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "chokidar": "^4.0.3",
    "esbuild": "^0.25.3",
    "eslint": "^9.25.1",
    "fs-extra": "^11.3.0",
    "glob": "^11.1.0",
    "js-yaml": "^4.1.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "vscode-oniguruma": "^2.0.1",
    "vscode-textmate": "^9.2.0"
  }
}
